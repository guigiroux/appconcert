<br><a href="http://localhost:3000/new"> 
	Creer un Concert
</a> 

<h3> Concerts </h3>
<ul>
  <% @concerts.each do |concert| %>
    <li>
		<p><%= concert.inspect %></p>
  
		<%= link_to "Show", concert_path(concert) %> <br/> 
		<%= link_to "Edit", edit_concert_path(concert) %><br/>
		<%= link_to "Destroy", concert_path(concert),
			method: :delete,
			data: {confirm: "Voulez-vous vraiment le supprimer?"} %>
	</li>
  <% end %>
</ul></br>



